{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "lib": ["DOM", "DOM.Iterable", "ES2021"],
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true,
        "strict": true,
        "target": "ES2021"
    },
    "include": ["src"]
}
